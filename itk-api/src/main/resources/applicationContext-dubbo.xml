                     check="false" version="1.0" url="" registry="zookeeper" protocol="dubbo" timeout="15000" />

    <dubbo:reference id="pageSortService" interface="com.itk.cms.service.PageSortService"
                     check="false" version="1.0" url="" registry="zookeeper" protocol="dubbo" timeout="15000" />

